{

  "description": "Shows wikipedia pages in two columns",
  "manifest_version": 2,
  "name": "Two Columns Wikipedia",
  "version": "1.0",
  "homepage_url": "https://github.com/arturolinares/two-cols-wikipedia",

  "background": {
    "scripts": ["background.js"]
  },

  "page_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },
  
  "permissions": [
    "activeTab",
    "tabs"
  ]

}